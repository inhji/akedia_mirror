<%= render SharedView, "posts_nav.html", conn: @conn, path_helper: &Routes.bookmark_path/2 %>

<div class="h-feed">
  <%= for bookmark <- @bookmarks do %>
    <%= render "bookmark.html", conn: @conn, bookmark: bookmark, current_user: @current_user %>
  <% end %>
</div>

<%= if Enum.empty?(@bookmarks) do %>
  <div>
    Doh!
  </div>
<% end %>
